# API Configuration - Staging
VITE_API_URL=https://staging-api.gastro-chatbot.com/api
VITE_API_TIMEOUT=15000

# Environment
VITE_NODE_ENV=staging

# Features
VITE_ENABLE_EMERGENCY_MODE=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ERROR_REPORTING=true

# Medical Settings
VITE_EMERGENCY_PHONE=911
VITE_POISON_CONTROL=01-800-0147-47
VITE_CRUZ_ROJA=066
VITE_MEDICAL_DISCLAIMER_URL=https://staging.gastro-chatbot.com/disclaimer

# Security
VITE_HIPAA_COMPLIANCE=true
VITE_ENCRYPTION_ENABLED=true
VITE_CSP_ENABLED=true
VITE_SECURE_COOKIES=false

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
VITE_DEFAULT_LANGUAGE=es

# Staging Settings
VITE_DEBUG_MODE=true
VITE_MOCK_API=false
VITE_ENABLE_PWA=false
VITE_ENABLE_OFFLINE_MODE=false

# Testing
VITE_E2E_TESTING=true
VITE_PERFORMANCE_TESTING=true
VITE_LOAD_TESTING=false

# Analytics & Monitoring
VITE_GOOGLE_ANALYTICS_ID=GA-STAGING-XXX
VITE_SENTRY_DSN=https://staging-sentry-dsn.ingest.sentry.io
VITE_HOTJAR_ID=STAGING-XXX

# Performance
VITE_ENABLE_COMPRESSION=false
VITE_BUNDLE_ANALYZER=true
VITE_PRELOAD_ROUTES=false

# Social & SEO
VITE_APP_NAME=GastroChatbot [STAGING]
VITE_APP_DESCRIPTION=Asistente médico inteligente para consultas gastroenterológicas [Entorno de Pruebas]
VITE_APP_URL=https://staging.gastro-chatbot.com
VITE_SOCIAL_IMAGE=https://staging.gastro-chatbot.com/og-image.png
