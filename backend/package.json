{
  "name": "gastro-chatbot-backend",
  "version": "1.0.0",
  "description": "Backend para chatbot de enfermedades gastrointestinales",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "db:seed": "tsx src/scripts/seed.ts",
    "seed": "node src/seed/run.js",
    "seed:clean": "node src/seed/run.js clean",
    "seed:prod": "node src/seed/run.js prod",
    "seed:verify": "node src/seed/run.js verify",
    "seed:stats": "node src/seed/run.js stats",
    "seed:help": "node src/seed/run.js help",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:reset": "prisma db push --force-reset"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "winston": "^3.10.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.0",
    "pg": "^8.11.3",
    "prisma": "^5.3.1",
    "@prisma/client": "^5.3.1",
    "openai": "^4.8.0",
    "axios": "^1.5.0",
    "zod": "^3.22.2",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "@types/bcryptjs": "^2.4.4",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/uuid": "^9.0.4",
    "@types/pg": "^8.10.2",
    "@types/compression": "^1.7.3",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
